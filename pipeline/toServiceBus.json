{
	"name": "toServiceBus",
	"properties": {
		"activities": [
			{
				"name": "Service Bus REST API",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": "https://abi-las-dev-brs-servicebus.servicebus.windows.net/abi-las-lte-v1.0-dev/messages",
					"method": "POST",
					"headers": {
						"CustomHeader-Version": "\"1.0\"",
						"Content-Type": "application/json",
						"BrokerProperties": {
							"value": "@concat('{\"CorrelationId\": \"',pipeline().RunId,'\"}')",
							"type": "Expression"
						}
					},
					"body": {
						"message": {
							"value": "@variables('OutputDetails')",
							"type": "Expression"
						}
					},
					"authentication": {
						"type": "MSI",
						"resource": "https://servicebus.azure.net/"
					}
				}
			}
		],
		"annotations": []
	}
}